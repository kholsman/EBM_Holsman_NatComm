      if(length(dim(x))>2){ #
        print("dim >2 not permited")#
        return(NA)#
      }else{#
        if(length(dim(x))==2){#
          out<-matrix(length(range),dim(x)[1],dim(x)[2])#
          for(r in 1:dim(out)[1]){#
            for(c in 1:dim(out)[2]){#
              if(is.na(x[r,c])){#
                out[r,c]<-NA#
              }else{#
                if(any(range>x[r,c]))#
                  out[r,c]<-which(range>x[r,c])[1]#
              }#
            }#
          }#
        }else{#
          nn<-length(x)#
          out<-rep(length(range),nn)#
          for(i in 1:nn){#
            if(is.na(x[i])){#
              out[i]<-NA#
            }else{	#
              if(any(range>x[i]))#
