plotresults2<-function(what="allR*100",ttmp=m1,newplot=1,ylimm=c(.2,.4),xlimm=c(startday1,endday1),coluse=col1,axis1=FALSE,alpha1=100){#
    # eval(parse(text=paste("ttmp<-",type,".Temp.all",sep="")))#
