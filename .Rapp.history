    legend("topright",c("3A","4CD"),col=cols,lty=ltyy,lwd=lwdd,pch=16,box.lty=0)#
    mtext(side=2,"Diet Energy",line=4.3)#
    mtext(side=2,expression(paste("(J ",g^-1,d^-1," prey )")),line=2.5)#
    plot_sub(var="tot_C_J_gpred",ylimm=c(0,150))#
