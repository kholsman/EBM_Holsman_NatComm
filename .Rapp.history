          tt<-tapply(subsub[,cc],subsub$Yr,mean,na.rm=TRUE)#
          tt.sd<-tapply(subsub[,cc],subsub$Yr,sd,na.rm=TRUE)#
          tt.n<-tapply(subsub[,cc],subsub$Yr,length.na)#
          tt.se<-abs(tt.sd/sqrt(tt.n))#
          ttu<-tt+1.95*(tt.se)#
          ttl<-tt-1.95*(tt.se)#
          xx.tt<-as.numeric(names(tt))#
