    plotresults2(alpha1=alpha,what="C*100",ttmp=CD4tmp,newplot=0,coluse=col2)	#
    mtext(expression(bold(paste("Consumption (% BW ",d^-1,")"))),side=2,line=2.5)#
    mtext("c)",side=3,font=2,cex=.9,adj=0.02,line=-1,outer=FALSE)#
