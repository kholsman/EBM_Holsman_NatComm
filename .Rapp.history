    plotdat<-dataset#
    lat<-plotdat$RLAT#
    lon<-plotdat$RLON#
    dat<-plotdat[,var]#
    W<-6;H<-4#
    mapcol1<-colorRampPalette(c(colors()[24],colors()[24]))#
    if(add1==FALSE)#
    {#
      dev.new(height=h,width=w)#
      # r<-ceiling(sqrt(length(plotYr)))#
      # c<-ceiling(length(plotYr)/r)#
      # par(mfrow=c(r,c))#
      par(mar=c(1,1,1,1)) # margins of graph: (bottom,left, top, right)#
      par(mgp=c(2,.5,0)) # axis margins: (distance of lab, distance of numbers, axis line)#
      par(oma=c(1,1,3,1))# outer margins of graph: (bottom,left, top, right)#
    } #
    if(is.na(range)){#
      range1<-round(c(min(dat,na.rm=TRUE),max(dat,na.rm=TRUE)),2)#
      range<-round(seq(range1[1],range1[2],(range1[2]-range1[1])/10),4)#
    }#
    coll2<-coll(length(range))#
    upto<-function(x){#
      if(length(dim(x))>2){ #
        print("dim >2 not permited")#
        return(NA)#
      }else{#
        if(length(dim(x))==2){#
          out<-matrix(length(range),dim(x)[1],dim(x)[2])#
          for(r in 1:dim(out)[1]){#
            for(c in 1:dim(out)[2]){#
              if(is.na(x[r,c])){#
                out[r,c]<-NA#
              }else{#
                if(any(range>x[r,c]))#
                  out[r,c]<-which(range>x[r,c])[1]#
              }#
            }#
          }#
        }else{#
          nn<-length(x)#
          out<-rep(length(range),nn)#
          for(i in 1:nn){#
            if(is.na(x[i])){#
              out[i]<-NA#
            }else{	#
              if(any(range>x[i]))#
                out[i]<-which(range>x[i])[1]#
            }#
          }#
        }#
        return(out)#
      }	#
    }#
    xmat_all<-list()#
    #for(yy in 1:length(plotYr)){#
    cc1<-which(plotdat$Yr%in% plotYr )#
    #	ylimm<-c(min(lat,na.rm=T),max(lat,na.rm=T))	#
    col1<-coll2[upto(dat[cc1])]#
    pch1<-rep(pchh,length(col1))#
    pch1[is.na(col1)]<-pch0#
    col1[is.na(col1)]<-nacol#
    cex1<-rep(cexx,length(col1))#
    cex1[is.na(col1)]<-cex1*.6#
    # plot(lat~lon,data=dat,col=FALSE,xlim=xlimm,ylim=ylimm)#
    # points(lat~lon,pch=pch1,col=col1,cex=cex1)#
    pp<-data.frame(lat=lat[cc1],lon=lon[cc1],dat=dat[cc1])#
    pp<-na.omit(pp)#
    nn<-nsplit#
    xo<-seq(-180,-130,(-130--180)/nn)#
    yo<-seq(50,62,(62-50)/nn)#
    sp_data <- interp(x = pp$lon,#
                      y = pp$lat,#
                      z = pp$dat,xo=xo,yo=yo,extrap=F,duplicate="mean")#
    # sp_data <- interp(x = pp$lon,#
    #                 y = pp$lat,#
    #                 z = pp$dat,xo=dattt[,1],yo=dattt[,2],extrap=F,duplicate="mean")#
    dInterp <- data.frame(expand.grid(x = sp_data$x,#
                                      y = sp_data$y), z = c(sp_data$z))#
    xmat<- xtabs(z~x+y, data=dInterp,exclude=NULL, na.action=na.pass)#
    xmat[xmat==0]<-NA#
    xmat_all<-xmat#
    range_val<-c(min(unlist(xmat_all),na.rm=TRUE),max(unlist(xmat_all),na.rm=TRUE))#
    dif<-(range_val[2]-range_val[1])/100#
    col_val<-seq(range_val[1]-dif,range_val[2]+dif,dif)#
    find_colval<-function(x){return(which(col_val>x)[1])}#
    # for(yy in 1:length(plotYr)){#
    xmat<-xmat_all#
    # ctmp<-matrix(NA,dim(xmat)[1],dim(xmat)[2])#
    # for(c in 1:dim(xmat)[2]){ctmp[,c]<-mapply(xmat[,c],FUN=find_colval)}#
    image(x=as.numeric(rownames(xmat)),y=as.numeric(colnames(xmat)),z=xmat,col=coll(length(col_val)-1),#
          add=FALSE,xlim=xlimm,ylim=ylimm,breaks=col_val)#
    ylimm2<-ylimm#
    ylimm2[1]<-ylimm[1]-4#
    ylimm2[2]<-ylimm[2]+4#
    xlimm2<-xlimm#
    xlimm2[1]<-xlimm[1]-4#
    xlimm2[2]<-xlimm[2]+4#
    map.fig_overlay(ylimm_lg=ylimm2,xlimm_lg=xlimm2)#
    mtext(side=3,outer=F,paste(var,plotYr[1],":",plotYr[length(plotYr)]),font=2,line=yrline,adj=yradj,cex=yrcex)#
    # if(yy==1) legend("topright",legend=rev(range),pch=pch1,col=rev(coll2),cex=cexx*legndscale)#
    # }#
    # if(byIndx==TRUE) #
    # 	mtext(side=3,outer=T,paste(model,Indx," weighted ",var,sep="; "),line=1,font=2)#
    # else#
    mtext(side=3,outer=T,paste(model,var,sep="; "),line=1,font=2)#
  }#
