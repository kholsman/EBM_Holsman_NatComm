fig5<-function(alpha=100,H=6,W=5){#
    dev.new(height=H,width=W)#
    par(mar=c(1,5,0,0)) # margins of graph: (bottom,left, top, right)#
    par(mgp=c(2,.5,0)) # axis margins: (distance of lab, distance of numbers, axis line)#
    par(oma=c(2,1,1,1))# outer margins of graph: (bottom,left, top, right)#
    par(mfrow=c(4,1))	#
    arealist<-c("East","West")#
    cols<-col1(6)[c(1,3,6)]#
    cols<-c(col1(5)[3],col1(5)[3])#
    ltyy=c(1,1,1);lwdd=c(2,2,2)#
    xat<-seq(1990,2015,5)#
    ylimm<-c(4000,5500)#
    plot_sub<-function(var="mnEDJ_g",ylimm=c(4000,5500),logg=FALSE){#
      cc<-grep(var,colnames(sub.preyED))#
      ### mean Energy density#
      for(i in 1:2){#
        if(i==1) subsub<-sub.4CD#
        if(i==2) subsub<-sub.3A#
        # subsub<-subsub1[grep(arealist[i],subsub1$ECOPATH_PP),]#
        tt<-tapply(subsub[,cc],subsub$Yr,mean,na.rm=TRUE)#
        tt.sd<-tapply(subsub[,cc],subsub$Yr,sd,na.rm=TRUE)#
        tt.n<-tapply(subsub[,cc],subsub$Yr,length.na)#
        tt.se<-abs(tt.sd/sqrt(tt.n))#
        yy<-as.numeric(names(tt))#
        ttu<-tt+1.95*(tt.se)#
        ttl<-tt-1.95*(tt.se)#
        bounds<-na.omit(data.frame(ttu,ttl))#
        yy<-rownames(bounds)#
        xx.tt<-as.numeric(names(tt))#
        if(logg==TRUE){#
          tt 		<-tapply(log(subsub[,cc]+0.001),subsub$Yr,mean,na.rm=TRUE)#
          tt.sd 	<-tapply(log(subsub[,cc]+0.001),subsub$Yr,sd,na.rm=TRUE)#
          tt.n 	<-tapply((subsub[,cc]),subsub$Yr,length.na)#
          tt.se 	<-abs(tt.sd/sqrt(tt.n))#
          yy 		<-as.numeric(names(tt))#
          ttu 	<-exp(tt+1.95*(tt.se))#
          ttl 	<-exp(tt-1.95*(tt.se))#
          tt 		<-exp(tt-0.001)#
          bounds<-na.omit(data.frame(ttu,ttl))#
          yy<-rownames(bounds)#
          xx.tt<-as.numeric(names(tt))#
        }#
        if(i==1){#
          plot(xx.tt,tt,ylab="",axes=FALSE,type="b",ylim=ylimm,pch=16,col=cols[i],lty=ltyy[i],lwd=lwdd[i])			#
          axis(2,las=2);axis(2,at=c(-1e5,1e5))#
          axis(1,at=xat,lab=rep("",length(xat)));axis(1,at=c(0,3000))#
          #abline(h=mean.na(subsub[,cc]),col=makeTransparent(cols[i],alpha=alpha1),lty=ltyy[i],lwd=lwdd[i])#
        }#
        polygon(c(yy,rev(yy)),c(bounds[,1],rev(bounds[,2])),col=makeTransparent(cols[i],alpha=alpha),border=F)#
        lines(xx.tt,tt,type="b",col=cols[i],pch=16,lty=ltyy[i],lwd=lwdd[i])#
        #abline(h=mean.na(subsub[,cc]),col=makeTransparent(cols[i],alpha=alpha1),lty=ltyy[i],lwd=lwdd[i])#
      }#
    }#
    plot_sub("mnEDJ_g",ylimm=c(4000,5500))#
    #legend("topright",arealist,col=cols,lty=ltyy,lwd=lwdd,box.lty=0)#
    legend("topright",c("3A","4CD"),col=cols,lty=ltyy,lwd=lwdd,pch=16,box.lty=0)#
    mtext(side=2,"Diet Energy",line=4.3)#
    mtext(side=2,expression(paste("(J ",g^-1,d^-1," prey )")),line=2.5)#
    plot_sub(var="tot_C_J_gpred",ylimm=c(0,150))#
    mtext(side=2,"Consumed Energy",line=4.3)#
    mtext(side=2,expression(paste("(J ",g^-1,d^-1," predator )")),line=2.5)#
    plot_sub("Pvalue",ylimm=c(0,1),logg=TRUE)	#
    mtext(side=2,"RFR",line=3)#
    # plot_sub("G_index",ylimm=c(-1,1))		#
    # 	mtext(side=2,"Growth efficiency",line=3)#
    # # plot_sub("G_hat",ylimm=c(-.001,.02))		#
    # 	mtext(side=2,"Growth ",line=4.3)#
    # 	mtext(side=2,expression(paste("(g ",g^-1,d^-1," predator )")),line=2.5)#
    # plot_sub("G_effic",ylimm=c(-50,50))	#
    # plot_sub("demand_ratio",ylimm=c(-50,50))		#
    #  	mtext(side=2,"Growth efficiency",line=3)	#
    plot_sub("G_scope",ylimm=c(0,.018))		#
    mtext(side=2,"Scope for growth",line=3)	#
    axis(1,at=xat)#
  }#
